    "ConnectionStrings": {
        "HotelListingDbConnetionString": "Server=(localdb)\\MSSQLLocalDB;Database=HotelListingAPIDb;Trusted_Connection=True;MultipleActiveResultSets=True"
    },
    "Serilog": {
        "MiniinLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "Path": "./logs/log-txt",
                    "rollingInterval": "Day"
                }
            },
            {
                "Name": "Seq",
                "Args": { "serverUrl": "http://localhost:5341" }
            }
        ]
    },
    "AllowedHosts": "*"
}
